{
  "name": "mcp-gbiz-info",
  "version": "0.0.4",
  "description": "ModelContext Protocol Server Gbiz INFO",
  "author": "tomiyan",
  "homepage": "https://github.com/tomiyan/mcp-gbiz-info",
  "bugs": "https://github.com/tomiyan/mcp-gbiz-info/issues",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.28.0",
  "bin": {
    "mcp-gbiz-info": "dist/index.js"
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "prepare": "pnpm run build",
    "watch": "tsc --watch",
    "publish-all": "pnpm publish --access public",
    "lint": "biome lint .",
    "format": "biome format . --write"
  },
  "engines": {
    "node": ">=24 <25"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.24.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@types/node": "^24",
    "shx": "^0.4.0",
    "typescript": "^5.9.3"
  }
}
